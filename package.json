{
  "name": "react-polymer-component",
  "version": "0.1.2",
  "description": "A generic React.Component for wrapping Polymer Elements and binding events and property data between them",
  "main": "PolymerComponent.js",
  "scripts": {
    "start": "npm run build && static-server",
    "build": "parcel build example/index.html --public-url ./ --out-dir example/build --no-cache --no-source-maps --no-minify",
    "build-test": "parcel build test/test-setup.html --public-url ./ --out-dir test/build --no-source-maps --no-minify",
    "lint": "eslint *.js",
    "test": "npm run build-test && jest"
  },
  "files": [
    "PolymerComponent.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gkjohnson/react-polymer-component.git"
  },
  "keywords": [
    "react",
    "polymer",
    "app",
    "javascript",
    "html",
    "web",
    "vdom",
    "webcomponents",
    "component",
    "polymer-element",
    "dom",
    "generic",
    "customelements",
    "custom-elements"
  ],
  "browserslist": [
    "last 1 Firefox versions"
  ],
  "author": "Garrett Johnson <garrett.kjohnson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gkjohnson/react-polymer-component/issues"
  },
  "homepage": "https://github.com/gkjohnson/react-polymer-component#readme",
  "devDependencies": {
    "@polymer/paper-button": "^3.0.0-pre.21",
    "@polymer/paper-card": "^3.0.0-pre.21",
    "@polymer/polymer": "^3.0.5",
    "@webcomponents/webcomponentsjs": "^2.0.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.4.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.6.0",
    "parcel-bundler": "^1.11.0",
    "puppeteer": "^1.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "static-server": "^2.2.1"
  }
}
